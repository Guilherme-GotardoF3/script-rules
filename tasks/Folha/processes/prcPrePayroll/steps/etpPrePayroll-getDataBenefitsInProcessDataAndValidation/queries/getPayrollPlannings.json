{
  "_id": "6376730ed1573700637b7129",
  "type": {
    "_id": "6376712d5ca21a0062379812",
    "name": "query"
  },
  "name": "getPayrollPlannings",
  "description": "Consulta o planejamento da folha de pagamento.",
  "main_collection": "payroll_plannings",
  "output_name": "opt_trf_buscar_planejamento_folha",
  "fixed_value": true,
  "input_parameters": [],
  "collections": [
    "plans",
    "investment_profiles",
    "monetary_unit_values"
  ],
  "output": {},
  "Aggregation": [
    {
      "$match": {
        "_id": "$.payrollPlanningId"
      }
    },
    {
      "$project": {
        "benefitTypes": 1,
        "competenceDate": 1,
        "payrollType": 1,
        "paymentDate": 1,
        "creditDate": 1,
        "plan": 1,
        "planId": "$plan._id",
        "status": 1
      }
    },
    {
      "$lookup": {
        "from": "plans",
        "localField": "plan._id",
        "foreignField": "_id",
        "as": "plan",
        "pipeline": [
          {
            "$project": {
              "investmentProfiles": 1
            }
          }
        ]
      }
    },
    {
      "$lookup": {
        "from": "investment_profiles",
        "localField": "plan.investmentProfiles._id",
        "foreignField": "_id",
        "as": "investment_profiles",
        "pipeline": [
          {
            "$project": {
              "monetaryUnits": 1,
              "_id": 0
            }
          }
        ]
      }
    },
    {
      "$lookup": {
        "from": "monetary_unit_values",
        "localField": "investment_profiles.monetaryUnits.monetaryUnit._id",
        "foreignField": "monetaryUnit._id",
        "as": "monetaryUnitValues",
        "let": {
          "filter_date": {
            "$dateToString": {
              "format": "%Y-%m-%d",
              "date": "$paymentDate"
            }
          },
          "planId": "$plan._id"
        },
        "pipeline": [
          {
            "$match": {
              "$expr": {
                "$and": [
                  {
                    "$lte": [
                      {
                        "$dateToString": {
                          "format": "%Y-%m-%d",
                          "date": "$referenceDate"
                        }
                      },
                      "$$filter_date"
                    ]
                  },
                  {
                    "$in": [
                      "$monetaryUnit.plan._id",
                      "$$planId"
                    ]
                  }
                ]
              }
            }
          },
          {
            "$sort": {
              "referenceDate": -1
            }
          },
          {
            "$limit": 1
          }
        ]
      }
    },
    {
      "$unwind": {
        "path": "$monetaryUnitValues",
        "preserveNullAndEmptyArrays": false
      }
    },
    {
      "$lookup": {
        "from": "monetary_unit_values",
        "localField": "investment_profiles.monetaryUnit._id",
        "foreignField": "monetaryUnit._id",
        "let": {
          "filter_date": {
            "$dateToString": {
              "format": "%Y-%m-%d",
              "date": "$competenceDate"
            }
          },
          "planId": "$plan._id"
        },
        "as": "preMonetaryUnitValues",
        "pipeline": [
          {
            "$match": {
              "$expr": {
                "$and": [
                  {
                    "$lt": [
                      {
                        "$dateToString": {
                          "format": "%Y-%m-%d",
                          "date": "$referenceDate"
                        }
                      },
                      "$$filter_date"
                    ]
                  },
                  {
                    "$in": [
                      "$monetaryUnit.plan._id",
                      "$$planId"
                    ]
                  }
                ]
              }
            }
          },
          {
            "$sort": {
              "referenceDate": -1
            }
          },
          {
            "$limit": 1
          }
        ]
      }
    },
    {
      "$unwind": {
        "path": "$preMonetaryUnitValues",
        "preserveNullAndEmptyArrays": true
      }
    },
    {
      "$project": {
        "investment_profiles": 0
      }
    }
  ]
}
