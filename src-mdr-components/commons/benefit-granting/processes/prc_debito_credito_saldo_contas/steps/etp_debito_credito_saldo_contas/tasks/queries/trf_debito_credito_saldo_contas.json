{
  "_id": "66476b723db1457a4c69b6c0",
  "type": {
    "_id": "66476b723637c09563323ba9",
    "name": "queries"
  },
  "name": "trf_debito_credito_saldo_contas",
  "description": "Tarefa que contabiliza debito e credito de contas do empregador",
  "main_collection": "evolucao_contas_empregador",
  "output_name": "otp_trf_debito_credito_saldo_contas",
  "fixed_value": false,
  "input_parameters": [],
  "collections": [
    "consulta_patrocinadoras",
    "consulta_evolucao_contas_empregador"
  ],
  "output": {},
  "Aggregation": [
    {
      "$match": {
        "$expr": {
          "$and": [
            {
              "$eq": [
                {
                  "$month": "$Data"
                },
                ".$mes"
              ]
            },
            {
              "$in": [
                "$Tipo da conta",
                [
                  "SA√çDAS",
                  "RENDIMENTO",
                  "ENTRADAS"
                ]
              ]
            }
          ]
        }
      }
    },
    {
      "$project": {
        "Nome do perfil": 0,
        "Data da cota": 0,
        "Valor da cota": 0,
        "Nome do plano": 0,
        "Ordem": 0,
        "Empregador": 0
      }
    },
    {
      "$lookup": {
        "from": "consulta_patrocinadoras",
        "localField": "Razao social",
        "foreignField": "Patroc reservas",
        "as": "patroc",
        "pipeline": [
          {
            "$limit": 1
          }
        ]
      }
    },
    {
      "$unwind": {
        "path": "$patroc",
        "preserveNullAndEmptyArrays": true
      }
    },
    {
      "$addFields": {
        "Data": {
          "$toDate": "31-10-2023"
        },
        "Plano": "$Cod cont",
        "rendimento": {
          "$cond": {
            "if": {
              "$eq": [
                "$Tipo da conta",
                "RENDIMENTO"
              ]
            },
            "then": {
              "$cond": {
                "if": {
                  "$gt": [
                    "$Valor da moeda",
                    0
                  ]
                },
                "then": "positivo",
                "else": "negativo"
              }
            },
            "else": null
          }
        },
        "Patrocinadora": {
          "$cond": {
            "if": {
              "$ifNull": [
                "$patroc.Patroc autopatrocinado",
                false
              ]
            },
            "then": "$patroc.Codigo",
            "else": "001"
          }
        }
      }
    },
    {
      "$group": {
        "_id": {
          "plano": "$Plano",
          "perfil": "$Perfil",
          "nome_conta": "$Nome da conta",
          "tipo_operacao": "$Tipo da operacao",
          "tipo_conta": "$Tipo da conta",
          "rendimento": "$rendimento"
        },
        "Data": {
          "$last": "$Data"
        },
        "Valor": {
          "$sum": "$Valor da moeda"
        },
        "D/C": {
          "$last": "D"
        },
        "CC": {
          "$last": 4
        },
        "Plano": {
          "$last": "$Plano"
        },
        "Perfil": {
          "$last": "$Perfil"
        },
        "Nome da conta": {
          "$last": "$Nome da conta"
        },
        "Tipo da operacao": {
          "$last": "$Tipo da operacao"
        },
        "Tipo da conta": {
          "$last": "$Tipo da conta"
        },
        "Patrocinadora": {
          "$last": "$Patrocinadora"
        }
      }
    },
    {
      "$lookup": {
        "from": "consulta_evolucao_contas_empregador",
        "localField": "Tipo da operacao",
        "foreignField": "Tipo da operacao",
        "let": {
          "nome_conta": "$Nome da conta",
          "tipo_conta": "$Tipo da conta"
        },
        "pipeline": [
          {
            "$match": {
              "$expr": {
                "$and": [
                  {
                    "$eq": [
                      "$Nome da conta",
                      "$$nome_conta"
                    ]
                  },
                  {
                    "$eq": [
                      "$Tipo da conta",
                      "$$tipo_conta"
                    ]
                  }
                ]
              }
            }
          }
        ],
        "as": "conta_contabil"
      }
    },
    {
      "$unwind": {
        "path": "$conta_contabil",
        "preserveNullAndEmptyArrays": false
      }
    },
    {
      "$project": {
        "_id": 0,
        "Nome da conta": 0,
        "Tipo da conta": 0,
        "Tipo da operacao": 0
      }
    },
    {
      "$facet": {
        "debito": [
          {
            "$addFields": {
              "Conta contabil": {
                "$cond": {
                  "if": {
                    "$eq": [
                      "$conta_contabil.Tipo da conta",
                      "RENDIMENTO"
                    ]
                  },
                  "then": {
                    "$cond": {
                      "if": {
                        "$gt": [
                          "$Valor",
                          0
                        ]
                      },
                      "then": "$conta_contabil.Conta debito",
                      "else": "$conta_contabil.Conta credito"
                    }
                  },
                  "else": {
                    "$cond": {
                      "if": {
                        "$eq": [
                          "$conta_contabil.Tipo da conta",
                          "ENTRADAS"
                        ]
                      },
                      "then": "$conta_contabil.Conta debito",
                      "else": "$conta_contabil.Conta debito"
                    }
                  }
                }
              },
              "Descricao": "$conta_contabil.Descricao"
            }
          },
          {
            "$project": {
              "conta_contabil": 0
            }
          }
        ],
        "credito": [
          {
            "$addFields": {
              "Conta contabil": {
                "$cond": {
                  "if": {
                    "$eq": [
                      "$conta_contabil.Tipo da conta",
                      "RENDIMENTO"
                    ]
                  },
                  "then": {
                    "$cond": {
                      "if": {
                        "$gt": [
                          "$Valor",
                          0
                        ]
                      },
                      "then": "$conta_contabil.Conta credito",
                      "else": "$conta_contabil.Conta debito"
                    }
                  },
                  "else": {
                    "$cond": {
                      "if": {
                        "$eq": [
                          "$conta_contabil.Tipo da conta",
                          "ENTRADAS"
                        ]
                      },
                      "then": "$conta_contabil.Conta credito",
                      "else": "$conta_contabil.Conta credito"
                    }
                  }
                }
              },
              "D/C": "C",
              "Descricao": "$conta_contabil.Descricao"
            }
          },
          {
            "$project": {
              "conta_contabil": 0
            }
          }
        ]
      }
    },
    {
      "$project": {
        "documentos": {
          "$concatArrays": [
            "$debito",
            "$credito"
          ]
        }
      }
    },
    {
      "$unwind": {
        "path": "$documentos"
      }
    },
    {
      "$project": {
        "Data": "$documentos.Data",
        "Conta contabil": "$documentos.Conta contabil",
        "Valor": {
          "$round": [
            {
              "$abs": "$documentos.Valor"
            },
            2
          ]
        },
        "D/C": "$documentos.D/C",
        "Descricao": "$documentos.Descricao",
        "CC": "$documentos.CC",
        "Plano": "$documentos.Plano",
        "Perfil": "$documentos.Perfil",
        "Patrocinadora": "$documentos.Patrocinadora"
      }
    },
    {
      "$merge": {
        "into": "registro_contabil_evolucao_contas_empregador",
        "on": [
          "Data",
          "Valor",
          "D/C",
          "Descricao",
          "Plano",
          "Perfil"
        ],
        "whenNotMatched": "insert"
      }
    }
  ]
}
